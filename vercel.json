{
    "version": 2,
    "builds": [
      {
        "src": "/.dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/.dist/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/.dist/index.js"
      }
    ]
  }